class HelloWorld {
    public static void main(String[] args) {
        // Utilizacin de la sentencia continue con una etiqueta.

exterior: for (int i=0; i<10; i++) {
for (int j=0; j<10; j++) {
if (j > i) {
System.out.println ();
continue exterior;
}
System.out.print (" " + (i * j));
}
}
System.out.println ();
}
}

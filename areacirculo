public class circulo {
    public static void main(String args[]) {
      
      double  radio= 2.3;
      double perimetro; 
      double area; 
      
      area=Math.PI*radio*radio;
      perimetro=2*Math.PI*radio;
      
      System.out.println("el area es: " + area +"el perimetro es:" + perimetro);

    }
}
